{
  "name": "my-nextjs-app",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.26.1",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "format": "prettier --write src",
    "format:check": "prettier --check src",
    "prepare": "husky"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "**/*.{json,md,html}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "next": "^15",
    "react": "^19",
    "react-dom": "^19"
  },
  "devDependencies": {
    "@commitlint/cli": "^20",
    "@eslint/eslintrc": "^3",
    "@tanstack/eslint-plugin-query": "^5",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8",
    "@typescript-eslint/parser": "^8",
    "eslint": "^9",
    "eslint-config-next": "^15",
    "eslint-config-prettier": "^10",
    "eslint-import-resolver-typescript": "^4",
    "eslint-plugin-import": "^2",
    "husky": "^9",
    "lint-staged": "^16",
    "prettier": "^3",
    "prettier-plugin-tailwindcss": "^0.7",
    "tailwindcss": "^4",
    "@tailwindcss/postcss": "^4",
    "typescript": "^5"
  }
}
